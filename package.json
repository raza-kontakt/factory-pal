{
  "name": "factory-pal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:run:report": "npx cypress run --reporter cypress-mochawesome-reporter",
    "cy:run:headless": "npx cypress run --headless",
    "cy:report:generate": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/combined-report.json && npx marge cypress/reports/combined-report.json --reportDir cypress/reports/html --inline",
    "cy:report:open": "find cypress/reports -name '*.html' -type f | head -1 | xargs open",
    "cy:report:clean": "rm -rf cypress/reports/*",
    "cy:test:full": "npm run cy:report:clean && npm run cy:run:report && npm run cy:report:open"
  },
  "dependencies": {
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/styled-engine-sc": "^7.2.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "axios": "^1.10.0",
    "i18next": "^25.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.0.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-jest": "^30.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cypress": "^14.5.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-bundle-analyzer": "^1.0.0"
  }
}
